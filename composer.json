{
  "name": "madebybramble/craft-google-shopping-feed",
  "type": "craft-plugin",
  "license": "proprietary",
  "support": {
    "email": "hello@madebybramble.co.uk"
  },
  "require": {
    "php": ">=8.2",
    "craftcms/cms": "^5.5.0",
    "craftcms/commerce": "^5.2"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "MadeByBramble\\GoogleShoppingFeed\\": "src/"
    }
  },
  "extra": {
    "handle": "google-shopping-feed",
    "name": "Google Shopping Feed",
    "developer": "Made By Bramble",
    "documentationUrl": "",
    "class": "MadeByBramble\\GoogleShoppingFeed\\Plugin"
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
